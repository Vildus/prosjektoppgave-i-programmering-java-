<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TableView fx:id="tvCustomerInventory" layoutX="36.0" layoutY="120.0" prefHeight="400.0" prefWidth="850.0">
            <columns>
                <TableColumn fx:id="colCategory" minWidth="-1.0" prefWidth="-1.0" text="Category" />
                <TableColumn fx:id="colArticleNumber" minWidth="-1.0" prefWidth="-1.0" text="Article Number" />
                <TableColumn fx:id="colBrand" minWidth="-1.0" prefWidth="-1.0" text="Brand" />
                <TableColumn fx:id="colModel" minWidth="-1.0" prefWidth="-1.0" text="Model" />
                <TableColumn fx:id="colPrice" minWidth="-1.0" prefWidth="-1.0" text="Price" />
                <TableColumn fx:id="colQty" minWidth="-1.0" prefWidth="-1.0" text="Qty" />
            </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        </TableView>
        <Button layoutX="783.0" layoutY="556.0" mnemonicParsing="false" onAction="#btnAddToShoppingBag" text="Shopping bag" />
        <HBox fx:id="hbNav" layoutX="-3.0" prefHeight="60.0" prefWidth="927.0" spacing="10.0">
            <children>
                <Button mnemonicParsing="false" onAction="#navGraphicCard" prefHeight="60.0" prefWidth="172.0" styleClass="menu-bar" stylesheets="@customer.css" text="Graphic Card" textOverrun="CLIP" />
                <Button mnemonicParsing="false" onAction="#navHardDisk" prefHeight="75.0" prefWidth="148.0" styleClass="menu-bar" stylesheets="@customer.css" text="Hard Disk" />
                <Button mnemonicParsing="false" onAction="#navKeyboard" prefHeight="60.0" prefWidth="151.0" styleClass="menu-bar" stylesheets="@customer.css" text="Keyboard" />
                <Button mnemonicParsing="false" onAction="#navMotherboard" prefHeight="60.0" prefWidth="172.0" styleClass="menu-bar" stylesheets="@customer.css" text="Motherboard" />
                <Button mnemonicParsing="false" onAction="#navMouse" prefHeight="72.0" prefWidth="148.0" styleClass="menu-bar" stylesheets="@customer.css" text="Mouse" />
                <Button mnemonicParsing="false" onAction="#navPowerSupply" prefHeight="60.0" prefWidth="172.0" styleClass="menu-bar" stylesheets="@customer.css" text="Power Supply" />
                <Button mnemonicParsing="false" onAction="#navProcessor" prefHeight="75.0" prefWidth="155.0" styleClass="menu-bar" stylesheets="@customer.css" text="Processor" />
                <Button mnemonicParsing="false" onAction="#navRAM" prefHeight="75.0" prefWidth="148.0" styleClass="menu-bar" stylesheets="@customer.css" text="RAM" />
                <Button mnemonicParsing="false" onAction="#navScreen" prefHeight="75.0" prefWidth="145.0" styleClass="menu-bar" stylesheets="@customer.css" text="Screen">
                    <cursor>
                        <Cursor fx:constant="DEFAULT" />
                    </cursor>
                </Button>
            </children>
            <padding>
                <Insets left="10.0" right="10.0" />
            </padding>
        </HBox>
        <Button layoutX="33.0" layoutY="556.0" mnemonicParsing="false" onAction="#btnAdmin" text="Admin" />
        <Label fx:id="lblNotifyMessage" layoutX="230.0" layoutY="583.0" prefHeight="27.0" prefWidth="395.0" />
    </children>
</AnchorPane>
